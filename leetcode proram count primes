class Solution {
    public int countPrimes(int n) {
int Rohi [] = new int[n];
for (int i=2; i <n; i++)
{
for(int j=i+i;j<n;j=j+i)
{
Rohi[j]=1;
}

    }
    int result=0;
    for(int i=2;i<Rohi.length;i++)
    {
        if(Rohi[i]==0)
        {
            result++;
        }
    }
      return result;
    }
}
